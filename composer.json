{
    "name": "turbo124/firebase-apns-notification",
    "description": "Laravel Push Notification",
    "keywords": [
        "laravel",
        "push notification",
        "notification",
        "laravel notifications"
    ],
    "homepage": "https://github.com/turbo124/firebase-apns-notification",
    "license": "MIT",
    "authors": [
        {
            "name": "David Bomba",
            "email": "turbo124@gmail.com"
        }
    ],
    "require": {
        "illuminate/config": "5.1.*|5.2.*|5.3.*",
        "illuminate/queue": "5.1.*|5.2.*|5.3.*",
        "sly/notification-pusher": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "Jenky\\LaravelPushNotification\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jenky\\LaravelNotification\\Test\\": "tests"
        }
    },
    "suggest": {
        "illuminate/notifications": "Required to use Laravel Notifications (~5.3)."
    },
    "scripts": {
        "test": "phpunit"
    }
}
